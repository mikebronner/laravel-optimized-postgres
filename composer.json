{
    "name": "genealabs/laravel-optimized-postgres",
    "description": "optimizes field types for Postgres.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "mike@genealabs.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelOptimizedPostgres\\": "src/",
            "GeneaLabs\\LaravelOptimizedPostgres\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelOptimizedPostgres\\Providers\\LaravelOptimizedPostgresService"
            ]
        }
    }
}
